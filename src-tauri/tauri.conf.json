{
	"build": {
		"beforeDevCommand": "",
		"beforeBuildCommand": "",
		"devPath": "../src",
		"distDir": "../src",
		"withGlobalTauri": true
	},
	"package": {
		"productName": "Solace Browser",
		"version": "0.1.0"
	},
	"tauri": {
		"allowlist": {
			"all": true
		},
		"bundle": {
			"active": true,
			"category": "DeveloperTool",
			"copyright": "",
			"deb": {
				"depends": []
			},
			"externalBin": [],
			"icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
			"identifier": "com.solace.browser",
			"longDescription": "A privacy-first, high-performance web browser built in Rust",
			"macOS": {
				"minimumSystemVersion": "10.13",
				"frameworks": [],
				"providerShortName": null,
				"signingIdentity": null,
				"entitlements": null,
				"exceptionDomain": "",
				"license": "LICENSE"
			},
			"resources": [],
			"shortDescription": "Solace Browser",
			"targets": "all",
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"security": {
			"csp": "default-src 'self' https: data: 'unsafe-inline' 'unsafe-eval'"
		},
		"macOSPrivateApi": true,
		"windows": [
			{
				"fullscreen": false,
				"height": 800,
				"resizable": true,
				"title": "Solace Browser",
				"width": 1200,
				"decorations": true,
				"transparent": false,
				"maximized": false,
				"theme": "Light"
			}
		]
	}
}
